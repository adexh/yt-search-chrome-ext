{
  "manifest_version": 3,
  "name": "Clickable Icons Extension",
  "version": "1.0",
  "description": "Adds clickable icons at the end of selected text.",
  "permissions": ["activeTab","storage"],
  "background": {
    "service_worker": "background.js"
  },  
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html"
  }
}
